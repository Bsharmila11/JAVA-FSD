public class MyThread1 implements Runnable
{
	 public void run()
	 {
	  		System.out.println("Runnaable Thread");
	 }
	 public static void main( String args[] )
	 {
	  		MyThread1 mt = new  MyThread1();
	  		Thread t=new Thread(mt);
	  		System.out.println(mt);
	  		t.setPriority(2);
	  		System.out.println(mt);
	  		t.start();
	 }
}


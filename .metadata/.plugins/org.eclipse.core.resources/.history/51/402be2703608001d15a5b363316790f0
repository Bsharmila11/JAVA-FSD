import java.util.*;
class TryCatch
{
	public static void main(String args[])
	{
		int a,b,c=0;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter two Numbers :");
		a=sc.nextInt();
		b=sc.nextInt();
		try
		{
			c=a/b;
			System.out.println(c);
		}
		catch(Exception e)
		{
			System.out.println("Exception Occured");
		}
		finally
		{
			System.out.println("Finally Block");
		}
		
	}
}

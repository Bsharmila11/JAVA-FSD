public class MyThread extends Thread
{
 	public void run()
 	{
  		System.out.println("Thread started Running..");
}
 	public static void main( String args[] )
 	{
  		MyThread mt = new  MyThread();
  		System.out.println(mt);
  		mt.setPriority(2);
  		System.out.println(mt);
  		mt.start();
 	}
}

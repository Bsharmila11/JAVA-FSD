import java.util.*;
public class Program26 
{
	public static void main(String args[])
	{
		int a=0,b=0,c=0,d=0;
		Scanner sc=new Scanner(System.in);
		int A[][]=new int[a][b];
		System.out.println("Enter the number of rows of matrix A ");
		a=sc.nextInt();
		System.out.println("Enter the number of columns of matrix A ");
		b=sc.nextInt();
		int B[][]=new int[c][d];
		System.out.println("Enter the number of rows of matrix B ");
		a=sc.nextInt();
		System.out.println("Enter the number of columns of matrix B ");
		b=sc.nextInt();
		System.out.println("Enter the elements of matrix A ");
		for (int i = 1; i < a; i++)
		{
            for (int j = 1; j < b; j++)
            {
                A[i][j] = sc.nextInt();
            }
		}
		System.out.println("Enter the elements of matrix B ");
		for (int i = 0; i < c; i++)
		{
            for (int j = 0; j < d; j++)
            {
                B[i][j] = sc.nextInt();
            }
		}
		System.out.println("Elements of the matrix A are");
        for (int i = 0; i < a; i++) 
        {
            for (int j = 0; j < b; j++)
            {
                System.out.print(A[i][j] + "  ");
            }
        }
        System.out.println("Elements of the matrix B are");
        for (int i = 0; i < c; i++) 
        {
            for (int j = 0; j < d; j++)
            {
                System.out.print(B[i][j] + "  ");
            }
        }
	}	
}

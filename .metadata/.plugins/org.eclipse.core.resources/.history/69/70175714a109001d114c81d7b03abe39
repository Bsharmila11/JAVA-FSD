package Java;
public class MyThread extends Thread
{
 	public void run()
 	{
  		System.out.println("Concurrent thread started running..");
}
 	public static void main( String args[] )
 	{
  		MyThread mt=new MyThread();
  		MyThread t=new MyThread();
  		t.setPriority(1);
  		System.out.println(mt);
  		System.out.println(mt);
  		t.start();
  		mt.start();
 	}
 }
